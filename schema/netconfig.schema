        SINGLE-VALUE )

#########################################################################################
# Package attributes
#########################################################################################

# brief descirption of the property
attributetype ( 1.3.6.1.4.1.214.1.80 NAME 'netconfigPackageSource'
        DESC 'source of the package'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 
        SINGLE-VALUE)

# version of the package
attributetype ( 1.3.6.1.4.1.214.1.81 NAME 'netconfigPackageVersion'
        DESC 'version of the package'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 
        SINGLE-VALUE)

# version of the package
attributetype ( 1.3.6.1.4.1.214.1.82 NAME 'netconfigPackageInstaller'
        DESC 'person who installed the package'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 
        SINGLE-VALUE)

##########################################################################################
# Object classes
# to update list of object classes, please use:
# grep '^objectclass' netconfig.schema| cut -f 3,5 -d ' ' | sed -e "s/^/# /" -e "s/'//g"
#
# 1.3.6.1.4.1.214.0.10 netconfigNode
# 1.3.6.1.4.1.214.0.11 netconfigClass
# 1.3.6.1.4.1.214.0.12 netconfigScript
# 1.3.6.1.4.1.214.0.13 netconfigMenu
# 1.3.6.1.4.1.214.0.14 netconfigPreseedConfig
# 1.3.6.1.4.1.214.0.15 netconfigKickstartConfig
# 1.3.6.1.4.1.214.0.16 netconfigAutoYastConfig
# 1.3.6.1.4.1.214.0.17 netconfigProperty
# 1.3.6.1.4.1.214.0.18 netconfigPackage
# 1.3.6.1.4.1.214.0.19 netconfigPreseedInstaller
# 1.3.6.1.4.1.214.0.20 netconfigFile
#########################################################################################

# object class for a network configuration node
objectclass ( 1.3.6.1.4.1.214.0.10 NAME 'netconfigNode' SUP top AUXILIARY
        DESC 'Node object for network configuration (e.g. computer, switch, etc)'
        MUST ( cn $ netconfigNodeType $ netconfigArchitecture )
        MAY ( netconfigName $ netconfigNodeAddress ) )

# object class for a network configuration class
objectclass ( 1.3.6.1.4.1.214.0.11 NAME 'netconfigClass' SUP top AUXILIARY
        DESC 'A set of nodes'
        MUST ( cn )
        MAY ( netconfigName $ netconfigArchitecture $ netconfigClassNode ) )

# object class for a network configuration script
objectclass ( 1.3.6.1.4.1.214.0.12 NAME 'netconfigScript' SUP top AUXILIARY
        DESC 'A set of nodes'
        MUST ( cn $ netconfigScriptType )
        MAY ( netconfigName $ netconfigArchitecture $ netconfigScriptData ) )

# object class for a network configuration boot menu
objectclass ( 1.3.6.1.4.1.214.0.13 NAME 'netconfigMenu' SUP top AUXILIARY
        DESC 'menu'
        MUST ( cn $ netconfigMenuTitle )
        MAY ( netconfigArchitecture $ netconfigMenuChild $ netconfigMenuKernel $ 
		      netconfigMenuPosition $ netconfigMenuBackground $ netconfigMenuAppend $ 
              netconfigMenuColor $ netconfigMenuLocal $ netconfigMenuFlags) )

# object class for a preseed configuration
objectclass ( 1.3.6.1.4.1.214.0.14 NAME 'netconfigPreseedConfig' SUP top AUXILIARY
        DESC 'preseed configuration'
        MUST ( cn )
        MAY ( netconfigName $ netconfigArchitecture $ netconfigPreseedAppend $ netconfigPreseedURL ) )

# object class for a kickstart configuration
objectclass ( 1.3.6.1.4.1.214.0.15 NAME 'netconfigKickstartConfig' SUP top AUXILIARY
        DESC 'Kickstart configuration (not implemented)'
        MUST ( cn )
        MAY ( netconfigName $ netconfigArchitecture ) )

# object class for a AutoYAST configuration
objectclass ( 1.3.6.1.4.1.214.0.16 NAME 'netconfigAutoYastConfig' SUP top AUXILIARY
        DESC 'AutoYAST configuration (not implemented)'
        MUST ( cn )
        MAY ( netconfigName $ netconfigArchitecture ) )

# object class for a netconfig property
objectclass ( 1.3.6.1.4.1.214.0.17 NAME 'netconfigProperty' SUP top AUXILIARY
        DESC 'configuration property'
        MUST ( cn $ netconfigPropertyType )
        MAY ( netconfigName $ netconfigArchitecture $ netconfigPropertyValue $ netconfigPropertyComment ) )


# object class for a package
objectclass ( 1.3.6.1.4.1.214.0.18 NAME 'netconfigPackage' SUP top AUXILIARY
        DESC 'package information'
        MUST ( cn )
        MAY ( netconfigName $ netconfigArchitecture $ netconfigPackageVersion $ description $ netconfigPackageInstaller $ netconfigPackageSource ) )

# object class for a preseed installer
objectclass ( 1.3.6.1.4.1.214.0.19 NAME 'netconfigPreseedInstaller' SUP top AUXILIARY
        DESC 'preseed installer'
        MUST ( cn )
        MAY ( netconfigName $ netconfigArchitecture $ netconfigInstallerURL $ netconfigInstallerFile  ) )

# object class for a file
objectclass ( 1.3.6.1.4.1.214.0.20 NAME 'netconfigFile' SUP top AUXILIARY
        DESC 'netconfig file'
        MUST ( cn $ netconfigFileData)
        MAY ( netconfigName $ netconfigArchitecture $ netconfigFileOwner $ netconfigFileGroup $ netconfigFilePermission ) )
