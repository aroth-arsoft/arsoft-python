        SINGLE-VALUE)

#########################################################################################
# Property attributes
#########################################################################################

# type of the property
attributetype ( 1.3.6.1.4.1.214.1.70 NAME 'netconfigPropertyType'
        DESC 'value of the property'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 
        SINGLE-VALUE)

# value of the property
attributetype ( 1.3.6.1.4.1.214.1.71 NAME 'netconfigPropertyValue'
        DESC 'value of the property'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )

# comment for the property
attributetype ( 1.3.6.1.4.1.214.1.72 NAME 'netconfigPropertyComment'
