#########################################################################################
# Common attributes
#########################################################################################
# name
attributetype ( 1.3.6.1.4.1.214.1.1 NAME 'netconfigName'
        DESC 'name of an item'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# architecture
attributetype ( 1.3.6.1.4.1.214.1.2 NAME 'netconfigArchitecture'
        DESC 'architecture of an item (i386, amd64, ppc, etc)'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

#########################################################################################
# Node attributes
#########################################################################################

# type of a node 
attributetype ( 1.3.6.1.4.1.214.1.10 NAME 'netconfigNodeType'
        DESC 'type of a node (can be computer, router, switch)'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# hardware address of a node (allow multiple addresses)
attributetype ( 1.3.6.1.4.1.214.1.11 NAME 'netconfigNodeAddress'
        DESC 'address of a node'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26)

#########################################################################################
# Class attributes
#########################################################################################

# link to a network node
attributetype ( 1.3.6.1.4.1.214.1.12 NAME 'netconfigClassNode'
        DESC 'link to a node'
        EQUALITY caseIgnoreMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.15)

#########################################################################################
# Script attributes
#########################################################################################

# octet string to store scripts (whole scripts file; bash or perl script)
attributetype ( 1.3.6.1.4.1.214.1.20 NAME 'netconfigScriptData'
        DESC 'script for network configuration'
        EQUALITY octetStringMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.40
        SINGLE-VALUE )

# type of a script
attributetype ( 1.3.6.1.4.1.214.1.21 NAME 'netconfigScriptType'
        DESC 'type of a script (can be main, update, etc)'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

#########################################################################################
# Menu attributes
#########################################################################################

# title of a menu 
attributetype ( 1.3.6.1.4.1.214.1.30 NAME 'netconfigMenuTitle'
        DESC 'menu title'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# link to another submenu
attributetype ( 1.3.6.1.4.1.214.1.31 NAME 'netconfigMenuChild'
        DESC 'link to a menu'
        EQUALITY caseIgnoreMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.15)

# name of the kernel
attributetype ( 1.3.6.1.4.1.214.1.32 NAME 'netconfigMenuKernel'
        DESC 'path to the kernel'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# kernel append line
attributetype ( 1.3.6.1.4.1.214.1.33 NAME 'netconfigMenuAppend'
        DESC 'kernel append line'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# background filename for the menu
attributetype ( 1.3.6.1.4.1.214.1.35 NAME 'netconfigMenuBackground'
        DESC 'background filename for the menu'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# position of the menu
attributetype ( 1.3.6.1.4.1.214.1.36 NAME 'netconfigMenuPosition'
        DESC 'position of the menu'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# position of the menu
attributetype ( 1.3.6.1.4.1.214.1.37 NAME 'netconfigMenuColor'
        DESC 'color of the menu'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# local boot
attributetype ( 1.3.6.1.4.1.214.1.38 NAME 'netconfigMenuLocal'
        DESC 'boot from local disk'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# flags for the menu item
attributetype ( 1.3.6.1.4.1.214.1.39 NAME 'netconfigMenuFlags'
        DESC 'flags for the menu item (e.g. preseed, autoyast, kickstart, etc)'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )

#########################################################################################
# Preseed attributes
#########################################################################################

# append for preseed
attributetype ( 1.3.6.1.4.1.214.1.50 NAME 'netconfigPreseedAppend'
        DESC 'append line'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

# URL for preseed
attributetype ( 1.3.6.1.4.1.214.1.51 NAME 'netconfigPreseedURL'
        DESC 'url for preseed config'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
        SINGLE-VALUE )

#########################################################################################
# Installer attributes
#########################################################################################

# URL for the installer
attributetype ( 1.3.6.1.4.1.214.1.52 NAME 'netconfigInstallerURL'
        DESC 'url for preseed config'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )

# name of the architecture
attributetype ( 1.3.6.1.4.1.214.1.54 NAME 'netconfigInstallerFile'
        DESC 'files from the installer archive'
        EQUALITY caseIgnoreIA5Match
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26)

